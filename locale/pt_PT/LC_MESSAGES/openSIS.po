msgid "Address"
msgstr "Endereço"

#: c://xampp/htdocs/opensis-lang/Help.php:94
msgid "Contacts"
msgstr "Contactos"

#: c://xampp/htdocs/opensis-lang/Help.php:95
msgid "Enrollment"
msgstr "Inscrição"

#: c://xampp/htdocs/opensis-lang/Help.php:95
